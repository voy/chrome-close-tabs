{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Close Tabs (Enhanced Edition)",
    "description": "Context menu to close in various clever ways.",
    "background": {
		"scripts": ["js/context_menu.js"]
    },
    "icons": {
        "128": "img/icon128.png",
        "16":  "img/icon16.png",
        "19":  "img/icon19.png",
        "32":  "img/icon32.png",
        "48":  "img/icon48.png"
    },
    "options_page": "options.htm",
    "permissions": ["*://*/*", "contextMenus", "tabs"],
    "update_url": "http://clients2.google.com/service/update2/crx"
}
