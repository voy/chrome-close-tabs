{
    "manifest_version": 2,
    "version": "1.0"
    "name": "Close Tabs",
    "description": "Context menu to close - tabs to the left, tabs to the right, other tabs, tabs from same domain, current tab, window and more",
    "homepage_url": "http://tejji.com/",
    "background": {
		"scripts": ["js/jquery.min.js", "js/options.js", "js/background.js"]
    },
    "icons": {
        "128": "img/icon128.png",
        "16":  "img/icon16.png",
        "19":  "img/icon19.png",
        "32":  "img/icon32.png",
        "48":  "img/icon48.png"
    },
    "options_page": "options.htm",
    "permissions": ["*://*/*", "contextMenus", "unlimitedStorage", "tabs"],
    "update_url": "http://clients2.google.com/service/update2/crx",
}
